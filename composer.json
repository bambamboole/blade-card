{
    "name": "bambamboole/blade-card",
    "description": "A Laravel Nova card to display Blade views.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Bambamboole\\BladeCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bambamboole\\BladeCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
